# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
tab_width = 4

#
[*.coffee]
indent_style = space
indent_size = 4

# Tab indentation (no size specified)
[*.js]
indent_style = tab
